	<queryString><![CDATA[SELECT 
CP.ENG_NAME,CP.THAI_NAME,CP.TH_ADDRESS1,CP.TH_ADDRESS2,CP.TH_ADDRESS3,CP.ADDRESS1 CP_ADDRESS1,CP.ADDRESS2 CP_ADDRESS2,CP.ADDRESS3 CP_ADDRESS3,CP.TELEPHONE CP_TELEPHONE,CP.FAX CP_FAX,
OHD.CUST_CODE,
OHD.DOC_NO,
OHD.DOC_DATE,
OHD.REF_NO,
OHD.REF_DATE,
OHD.REMARKS,
OHD.VALIDITY,
OHD.DESCS,
OHD.FAX_TO,
OHD.CC_TO,
OHD.TERM_OF_PAYMENT,
ODT.UOM,
ODT.UNIT_COST,
ODT.QTY,
ODT.REMARKS AS ITEM_REMARK,
ODT.MARK_UP,
CUS.NAME,
cus.fax_no,
CUS.TELEPHONE,
CASE WHEN  CUS.ADDRESS1 IS NULL THEN '' ELSE CUS.ADDRESS1 END AS ADDRESS1,
CASE WHEN CUS.ADDRESS2 IS NULL THEN '' ELSE CUS.ADDRESS2 END AS ADDRESS2,
CASE WHEN CUS.ADDRESS3 IS NULL THEN '' ELSE CUS.ADDRESS3 END AS ADDRESS3,
CASE WHEN CUS.POSTCODE IS NULL THEN '' ELSE CUS.POSTCODE END AS POSTCODE,
CASE WHEN CUS.PROVINCE IS NULL THEN '' ELSE CUS.PROVINCE END AS PROVINCE,ADT.CREDIT_TERM,
ITM.ITEM_CODE,
ITM.DESCS AS ITEM_DESCS,
odt.line_no,
SO_COMPONENTS_DETAIL.line1 ,
SO_COMPONENTS_DETAIL.line2,
SO_COMPONENTS_DETAIL.line3, 
SO_COMPONENTS_DETAIL.line4, 
SO_COMPONENTS_DETAIL.line5, 
SO_COMPONENTS_DETAIL.line6, 
OHD.CURRENCY_CD,
CURRENCY.DESCS AS CURR_DESCS
FROM SO_SALES_ITEM_HD ITM,COMPANY_PROFILE CP,
AR_DEBTOR ADT,
SO_CUSTOMERS CUS,
SO_QUOTATION_HD OHD,
SO_QUOTATION_DT ODT,
SO_SALES_ITEM_REQ_SHEET  ,
CURRENCY,
SO_COMPONENTS_DETAIL
WHERE 
   (
      (OHD.DOC_NO = ODT.DOC_NO)
   and 
      (OHD.DOC_DATE = ODT.DOC_DATE)
   and 
      (OHD.CUST_CODE = ODT.CUST_CODE)
   and 
      (OHD.CUST_CODE = CUS.CUSTOMER_CD)
   and 
      (ODT.ITEM_CODE = ITM.ITEM_CODE)
   and 
      ( ADT.CURRENCY_CODE = CURRENCY.CURRENCY_CODE   )
   AND
      (CUS.DEBTOR_ACCT = ADT.DEBTOR_ACCT)
   and 
      (SO_SALES_ITEM_REQ_SHEET.CUST_CODE = ODT.CUST_CODE)
   and 
      (SO_SALES_ITEM_REQ_SHEET.ITEM_CODE = ODT.ITEM_CODE)
   and 
      (SO_SALES_ITEM_REQ_SHEET.UOM_CD = ODT.UOM)
   and 
      (SO_SALES_ITEM_REQ_SHEET.CUST_CODE = OHD.CUST_CODE)
   and 
      (SO_SALES_ITEM_REQ_SHEET.CUST_CODE = CUS.CUSTOMER_CD)
   and
    (SO_SALES_ITEM_REQ_SHEET.CUST_CODE = ITM.CUST_CODE)
   and 
      (SO_SALES_ITEM_REQ_SHEET.ITEM_CODE = ITM.ITEM_CODE)
   and 
      (SO_SALES_ITEM_REQ_SHEET.UOM_CD = ITM.UOM_CD)
    and
    (SO_COMPONENTS_DETAIL.item_code = SO_SALES_ITEM_REQ_SHEET.ITEM_CODE)
    AND
    (SO_COMPONENTS_DETAIL.CUST_CODE = SO_SALES_ITEM_REQ_SHEET.CUST_CODE)
    AND
(OHD.DOC_NO =$P{pDocNo})

   )]]></queryString>

