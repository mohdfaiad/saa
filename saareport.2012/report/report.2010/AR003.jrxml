	<queryString><![CDATA[SELECT AR_DEBTOR.DEBTOR_ACCT, AR_DEBTOR.NAME, 
AR_DEBTOR.DEBTOR_TYPE, 
AR_DEBTOR.CREDIT_TERM, 
AR_DEBTOR.CURRENCY_CODE, 
AR_LEDGER.DOC_NO, 
AR_LEDGER.DOC_DATE, 
AR_LEDGER.REF_NO, 
AR_LEDGER.REF_DATE, 
AR_LEDGER.TRX_TYPE, AR_LEDGER.DESCS, 
AR_LEDGER.CURRENCY_RATE, AR_LEDGER.MBASE_AMT, AR_LEDGER.MTAX_AMT,
 round(AR_LEDGER.MDOC_AMT,2) as MDOC_AMT, AR_LEDGER.FBASE_AMT, AR_LEDGER.FTAX_AMT, AR_LEDGER.FDOC_AMT, 
AR_LEDGER.MBAL_AMT,
round(AR_LEDGER.FBAL_AMT,2) AS FBAL_AMT,AR_LEDGER.MALLOC_AMT, AR_LEDGER.CLASS, AR_LEDGER.UPDATE_USER, AR_LEDGER.UPDATE_DATE,AR_LEDGER.BANK_CD
FROM AR_DEBTOR
   INNER JOIN AR_LEDGER ON (AR_DEBTOR.DEBTOR_ACCT = AR_LEDGER.DEBT_ACCT)
WHERE CLASS  IN ('I'  )
and fbal_amt >0
and doc_date <=$P{pToDate}
and ar_debtor.NAME =$P{pDebtorAcct}
UNION
SELECT AR_DEBTOR.DEBTOR_ACCT, AR_DEBTOR.NAME, 
AR_DEBTOR.DEBTOR_TYPE, 
AR_DEBTOR.CREDIT_TERM, 
AR_DEBTOR.CURRENCY_CODE, 
AR_LEDGER.DOC_NO, 
AR_LEDGER.DOC_DATE, 
AR_LEDGER.REF_NO, 
AR_LEDGER.REF_DATE, 
AR_LEDGER.TRX_TYPE, AR_LEDGER.DESCS, 
AR_LEDGER.CURRENCY_RATE, AR_LEDGER.MBASE_AMT, AR_LEDGER.MTAX_AMT,
 round(AR_LEDGER.MDOC_AMT,2) as MDOC_AMT, AR_LEDGER.FBASE_AMT, AR_LEDGER.FTAX_AMT, AR_LEDGER.FDOC_AMT, 
AR_LEDGER.MBAL_AMT,
round(AR_LEDGER.FBAL_AMT,2) AS FBAL_AMT,AR_LEDGER.MALLOC_AMT, AR_LEDGER.CLASS, AR_LEDGER.UPDATE_USER, AR_LEDGER.UPDATE_DATE,AR_LEDGER.BANK_CD
FROM AR_DEBTOR
   INNER JOIN AR_LEDGER ON (AR_DEBTOR.DEBTOR_ACCT = AR_LEDGER.DEBT_ACCT)
WHERE CLASS  IN ('C' ,'D'  )
AND AR_LEDGER.MALLOC_AMT =0
and doc_date <=$P{pToDate}
and ar_debtor.NAME =$P{pDebtorAcct}]]></queryString>

