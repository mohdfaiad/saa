   and 
      (SO_SALES_ITEM_REQ_SHEET.CUST_CODE = CUS.CUSTOMER_CD)
   and
    (SO_SALES_ITEM_REQ_SHEET.CUST_CODE = ITM.CUST_CODE)
   and 
      (SO_SALES_ITEM_REQ_SHEET.ITEM_CODE = ITM.ITEM_CODE)
   and 
      (SO_SALES_ITEM_REQ_SHEET.UOM_CD = ITM.UOM_CD)
   and 
