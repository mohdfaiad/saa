	<queryString><![CDATA[SELECT SO_ORDER_HD.CUST_CODE,
 SO_ORDER_HD.ORDER_NO,
 SO_ORDER_HD.ORDER_DATE,
 SO_ORDER_HD.REF_NO, 
SO_ORDER_HD.REF_DATE,
so_order_hd.REMARKS AS HREMARK,
CASE SO_ORDER_HD.STATUS WHEN 'A' then 'Approved'
when 'C' then 'Cancel' when 'O' then 'Closed'
end AS ORDER_STATUS,
SO_ORDER_HD.ORDER_AMT,
SO_ORDER_HD.DESCS, 
SO_ORDER_HD.SCHEDULE_DATE,
SO_ORDER_HD.FAX_TO,
SO_ORDER_HD.term_of_payment,
SO_ORDER_HD.CC_TO, SO_ORDER_HD.NO_OF_PRINT,
SO_ORDER_DT.ITEM_CODE, SO_ORDER_DT.LINE_NO, SO_ORDER_DT.UOM, SO_ORDER_DT.ITEM_DESCS, SO_ORDER_DT.UNIT_COST, 
SO_ORDER_DT.TOTAL_AMT, SO_ORDER_DT.REMARKS, SO_ORDER_DT.DISCS_AMT, SO_ORDER_DT.BASE_AMT, SO_ORDER_DT.TAX_AMT, 
SO_ORDER_DT.TAX_CD, SO_ORDER_DT.DISCS_CD, SO_ORDER_DT.QTY, SO_ORDER_DT.TAX_RATE,


CASE SO_ORDER_DT.STATUS WHEN 'A' then 'Approved'
when 'C' then 'Cancel' when 'O' then 'Closed'
end AS ITEM_STATUS,
SO_ORDER_DT.DET_QTY, SO_ORDER_DT.DELIVERY_DATE, SO_CUSTOMERS.NAME

FROM SO_ORDER_DT  , SO_ORDER_HD , SO_CUSTOMERS
WHERE  (SO_ORDER_DT.CUST_CODE = SO_ORDER_HD.CUST_CODE)
AND (SO_ORDER_DT.ORDER_NO = SO_ORDER_HD.ORDER_NO) 
AND (SO_ORDER_DT.ORDER_DATE = SO_ORDER_HD.ORDER_DATE)
AND (SO_ORDER_HD.CUST_CODE = SO_CUSTOMERS.CUSTOMER_CD)
AND (SO_ORDER_HD.ORDER_DATE >= $P{pStartDate})
and  (SO_ORDER_HD.ORDER_DATE <= $P{pEndDate})
and (so_order_hd.cust_code =$P{pCustCode})]]></queryString>

